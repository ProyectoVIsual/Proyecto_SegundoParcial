<Window x:Class="ReporteGeneral"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto_Segundo_Parcial"
        mc:Ignorable="d"
        Title="ReporteGeneral" Height="300" Width="648.718">
    <Grid>
        <DataGrid x:Name="dataGrid1" HorizontalAlignment="Left" Height="216" Margin="10,10,-335,0" VerticalAlignment="Top" Width="617" AutoGenerateColumns="False" ItemsSource="{Binding candidato}" EnableRowVirtualization="False" >
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding nombre}" Header="Nombre"/>
                <DataGridTextColumn Binding="{Binding apellido}" Header="Apellido"/>
                <DataGridTextColumn Binding="{Binding lista}" Header="Lista"/>
                <DataGridTextColumn Binding="{Binding Votos}" Header="Votos"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button x:Name="btnCerrarReporteG" Content="Cerrar" HorizontalAlignment="Left" Height="27" Margin="489,233,-335,0" VerticalAlignment="Top" Width="138"/>

    </Grid>
</Window>
